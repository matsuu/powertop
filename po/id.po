#: powertop.c:460
#, c-format
msgid "C0 (cpu running)        (%4.1f%%)\n"
msgstr "C0 (cpu bekerja)        (%4.1f%%)\n"

#: powertop.c:459
#, c-format
msgid "Cn\t    Avg residency (%is)\tLong term residency avg\n"
msgstr "Cn\t    Residensi rata2 (%is)\tResidensi jangka panjang rata2\n"

#: powertop.c:400
