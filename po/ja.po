#: misctips.c:172
msgid " A - Turn AC97 powersave on "
msgstr " A - AC97の省電力モードを有効にする "

#: bluetooth.c:130
msgid " B - Turn Bluetooth off "
msgstr " B - Bluetoothを無効にする "

#: misctips.c:263
msgid " C - Power aware CPU scheduler "
msgstr "C - 省電力のCPUスケジューラに設定する "

#: wireless.c:208
msgid " I - disable WIFI Radio "
msgstr " I - 無線LANを無効にする "

#: misctips.c:80
msgid " L - enable Laptop mode "
msgstr " L - ラップトップモードを有効にする "

#: misctips.c:111
msgid " N - Turn NMI watchdog off "
msgstr " N - NMI監視を無効にする "

#: cpufreq.c:118
msgid " O - enable Ondemand governor "
msgstr " O - オンデマンド周波数制御を有効にする "

#: display.c:107
msgid " Q - Quit "
msgstr " Q - 終了 "

#: display.c:108
msgid " R - Refresh "
msgstr " R - 更新 "

#: sata.c:98
msgid " S - SATA Link Power Management "
msgstr " S - SATA電源管理を有効にする "

#: misctips.c:201
msgid " T - enable noatime "
msgstr " T - noatimeを有効にする "

#: wireless.c:203
msgid " W - Enable wireless power saving "
msgstr " W- 無線の省電力モードを有効にする "

